{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_USE_HERMES": "true"
        },
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
